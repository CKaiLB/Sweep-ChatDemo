{
  "version": 2,
  "projectSettings": {
    "framework": null,
    "buildCommand": "npm install && node scripts/build-config.js && cp -r public/* . && cp -r demos/* .",
    "outputDirectory": ".",
    "installCommand": null,
    "devCommand": null
  },
  "routes": [
    {
      "src": "/",
      "dest": "/demos/marketplace/index.html"
    },
    {
      "src": "/api/webhook/chat",
      "dest": "/api/webhook/chat.js"
    },
    {
      "src": "/demos/(.*)",
      "dest": "/demos/$1",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
} 